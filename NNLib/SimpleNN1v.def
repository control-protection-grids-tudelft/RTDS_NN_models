Component Builder 2.0
COMPONENT-BUILDER-OPTIONS:
  COMPONENT-MODE:        CTL
  AUTO-UPDATE-FILE-DEFN: true
  SHOW-NODE-NAMES:       false
  DEFAULT-FONT-SIZE:     10

PARAMETERS:
  SECTION: "CONFIGURATION"
    CtrlGrp "Assigned Controls Group" "" 0 INTEGER 1 1 36
    Pri     "Priority Level"          "" 0 INTEGER 1 1
  SECTION: "SimpleNN1" FALSE
    numInputs  "Number of Inputs:"  "" 0 INTEGER 7 7 7
    numOutputs "Number of Outputs:" "" 0 INTEGER 1 1 1

GRAPHICS:
  Box(-32,-128,32,128)
  Arc(0,-128,4,0,-180)
  Line(-64,-96,-32,-96)
  Line(-32,-96,-40,-104)
  Line(-32,-96,-40,-88)
  FontSize(9)
  FText(-69.0,-86,"x1")
  If (numInputs>1)
    Line(-64,-64,-32,-64)
    Line(-32,-64,-40,-72)
    Line(-32,-64,-40,-56)
    FontSize(9)
    FText(-69.0,-54,"x2")
  EndIf
  If (numInputs>2)
    Line(-64,-32,-32,-32)
    Line(-32,-32,-40,-40)
    Line(-32,-32,-40,-24)
    FontSize(9)
    FText(-69.0,-22,"u1")
  EndIf
  If (numInputs>3)
    Line(-64,0,-32,0)
    Line(-32,0,-40,-8)
    Line(-32,0,-40,8)
    FontSize(9)
    FText(-69.0,10,"Lr")
  EndIf
  If (numInputs>4)
    Line(-64,32,-32,32)
    Line(-32,32,-40,24)
    Line(-32,32,-40,40)
    FontSize(9)
    FText(-69.0,42,"id")
  EndIf
  If (numInputs>5)
    Line(-64,64,-32,64)
    Line(-32,64,-40,56)
    Line(-32,64,-40,72)
    FontSize(9)
    FText(-64.0,74,"b")
  EndIf
  If (numInputs>6)
    Line(-64,96,-32,96)
    Line(-32,96,-40,88)
    Line(-32,96,-40,104)
    FontSize(9)
    FText(-64.0,106,"a")
  EndIf
  Line(64,0,32,0)
  FontSize(9)
  FText(64.0,10,"y")
  FontSize(10)
  FText(0,136,"Time Step")
  FontSize(10)
  FText(0,152,"specified by ")
  FontSize(10)
  FText(0,168,"Simulink =")
  FontSize(10)
  FText(0,184,"none")

NODES:
  INITIALIZE_NODE x2 = _dud_
  INITIALIZE_NODE u1 = _dud_
  INITIALIZE_NODE Lr = _dud_
  INITIALIZE_NODE id = _dud_
  INITIALIZE_NODE b  = _dud_
  INITIALIZE_NODE a  = _dud_
  INITIALIZE_NODE y  = _dud_
  INITIALIZE_NODE x1 = _dud_

  #IF numInputs>=2
    x2 -2 -2 INPUT REAL
  #END
  #IF numInputs>=3
    u1 -2 -1 INPUT REAL
  #END
  #IF numInputs>=4
    Lr -2 0 INPUT REAL
  #END
  #IF numInputs>=5
    id -2 1 INPUT REAL
  #END
  #IF numInputs>=6
    b -2 2 INPUT REAL
  #END
  #IF numInputs>=7
    a -2 3 INPUT REAL
  #END
  y 2 0 OUTPUT REAL
  x1 -2 -3 INPUT REAL

VALVE-GROUP:
  RTDS.CTL.RISC_CMODEL
      V4COMP: ModelName = SimpleNNv SubSystem = $SS Processor = $ctrlGrp Priority = $Pri
      DATA_SIZES: INTDATA=2 FLOATDATA=0 CHARDATA_FRMT=0 INVARS=7 OUTVARS=1
      INTDATA: $numInputs numInputs $numOutputs numOutputs
      INVARS: $x2 IEEE x2 $u1 IEEE u1 $Lr IEEE Lr $id IEEE id $b IEEE b
      INVARS: $a IEEE a $x1 IEEE x1
      OUTVARS: $y IEEE y
  END

COMPONENT-DESCRIPTORS:
  CLASSIFICATION: GENERAL
  PHASES:         N/A
  MAINSTEP:       YES
  SUBSTEP:        NO
  SUPERSTEP:      NO
  SMALLDT:        NO
  GPES:           NO
  TWRT:           NO
  DIST:           NO
  DEPRECATED:     NO

