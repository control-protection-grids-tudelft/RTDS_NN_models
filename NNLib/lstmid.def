Component Builder 2.0
COMPONENT-BUILDER-OPTIONS:
  COMPONENT-MODE:        CTL
  AUTO-UPDATE-FILE-DEFN: true
  SHOW-NODE-NAMES:       false
  DEFAULT-FONT-SIZE:     10

PARAMETERS:
  SECTION: "CONFIGURATION"
    CtrlGrp "Assigned Controls Group" "" 0 INTEGER 1 1 36
    Pri     "Priority Level"          "" 0 INTEGER 1 1
  SECTION: "lstmid" FALSE
    numInputs  "Number of Inputs:"  "" 0 INTEGER 4 4 4
    numOutputs "Number of Outputs:" "" 0 INTEGER 1 1 1

GRAPHICS:
  Box(-32,-96,32,96)
  Arc(0,-96,4,0,-180)
  Line(-64,-64,-32,-64)
  Line(-32,-64,-40,-72)
  Line(-32,-64,-40,-56)
  FontSize(9)
  FText(-69.0,-54,"x1")
  If (numInputs>1)
    Line(-64,-32,-32,-32)
    Line(-32,-32,-40,-40)
    Line(-32,-32,-40,-24)
    FontSize(9)
    FText(-69.0,-22,"x2")
  EndIf
  If (numInputs>2)
    Line(-64,0,-32,0)
    Line(-32,0,-40,-8)
    Line(-32,0,-40,8)
    FontSize(9)
    FText(-64.0,10,"l")
  EndIf
  If (numInputs>3)
    Line(-64,32,-32,32)
    Line(-32,32,-40,24)
    Line(-32,32,-40,40)
    FontSize(9)
    FText(-69.0,42,"Lr")
  EndIf
  Line(64,0,32,0)
  FontSize(9)
  FText(74.0,10,"y_hat")
  FontSize(10)
  FText(0,104,"Time Step")
  FontSize(10)
  FText(0,120,"specified by ")
  FontSize(10)
  FText(0,136,"Simulink =")
  FontSize(10)
  FText(0,152,"none")

NODES:
  INITIALIZE_NODE x2    = _dud_
  INITIALIZE_NODE l     = _dud_
  INITIALIZE_NODE Lr    = _dud_
  INITIALIZE_NODE y_hat = _dud_
  INITIALIZE_NODE x1    = _dud_

  #IF numInputs>=2
    x2 -2 -1 INPUT REAL
  #END
  #IF numInputs>=3
    l -2 0 INPUT REAL
  #END
  #IF numInputs>=4
    Lr -2 1 INPUT REAL
  #END
  y_hat 2 0 OUTPUT REAL
  x1 -2 -2 INPUT REAL

VALVE-GROUP:
  RTDS.CTL.RISC_CMODEL
      V4COMP: ModelName = lstmid SubSystem = $SS Processor = $CtrlGrp Priority = $Pri
      DATA_SIZES: INTDATA=2 FLOATDATA=0 CHARDATA_FRMT=0 INVARS=4 OUTVARS=1
      INTDATA: $numInputs numInputs $numOutputs numOutputs
      INVARS: $Lr IEEE Lr $l IEEE l $x2 IEEE x2 $x1 IEEE x1
      OUTVARS: $y_hat IEEE y_hat
  END

COMPONENT-DESCRIPTORS:
  CLASSIFICATION: GENERAL
  PHASES:         N/A
  MAINSTEP:       YES
  SUBSTEP:        NO
  SUPERSTEP:      NO
  SMALLDT:        NO
  GPES:           NO
  TWRT:           NO
  DIST:           NO
  DEPRECATED:     NO

