Component Builder 2.0
COMPONENT-BUILDER-OPTIONS:
  COMPONENT-MODE:        CTL
  AUTO-UPDATE-FILE-DEFN: true
  SHOW-NODE-NAMES:       false
  DEFAULT-FONT-SIZE:     10

PARAMETERS:
  SECTION: "CONFIGURATION"
    ctrlGrp "Assigned Control Group" "" 0 INTEGER 1 1 54
    Pri     "Priority Level"         "" 0 INTEGER 1 1
  SECTION: "ParameterSelection"
    numInputs  "Description" ""                    0 INTEGER 1 1 10
    numOutputs "Description" ""                    0 INTEGER 1 1 10
    activation "Description" "LINEAR;TAH;RELU;SIG" 0 TOGGLE  0
    batch_size "Description" ""                    0 INTEGER 1

GRAPHICS:
  Box(-32,-280,64,480)
  Line(56,-40,56,-40)
  Line(56,-40,56,-40)
  Line(56,-40,56,-40)
  Line(56,-40,56,-40)
  Line(-32,-256,-96,-256)
  Line(-32,416,-96,416)
  FontSize(10)
  FText(-72,432,"Learning Rate")
  FontSize(10)
  FText(-72,-248,"Input1")
  Line(-32,96,-96,96)
  FontSize(10)
  FText(-80,104,"L1")
  Line(64,-224,128,-224)
  FontSize(10)
  FText(112,-216,"y1")
  Line(-32,448,-96,448)
  FontSize(10)
  FText(-72,456,"lambda")
  If (numInputs > 1)
    Line(-32,-224,-96,-224)
    FontSize(10)
    FText(-72,-216,"Input 2")
    If (numInputs > 2)
      Line(-32,-192,-96,-192)
      FontSize(10)
      FText(-72,-184,"Input3")
      If (numInputs>3)
        Line(-32,-160,-96,-160)
        FontSize(10)
        FText(-72,-152,"input4")
        If (numInputs>4)
          Line(-32,-128,-88,-128)
          FontSize(10)
          FText(-72,-120,"input5")
          If (numInputs>5)
            Line(-32,-96,-88,-96)
            FontSize(10)
            FText(-72,-88,"input6")
            If (numInputs>6)
              Line(-32,-64,-96,-64)
              FontSize(10)
              FText(-72,-56,"Input7")
              If (numInputs>7)
                FontSize(10)
                FText(-72,-24,"Input 8")
                Line(-32,-32,-96,-32)
                If (numInputs>8)
                  Line(-32,0,-96,0)
                  FontSize(10)
                  FText(-72,8,"Input 9")
                  If (numInputs>9)
                    Line(-32,32,-96,32)
                    FontSize(10)
                    FText(-72,40,"input 10")
                  EndIf
                EndIf
              EndIf
            EndIf
          EndIf
        EndIf
      EndIf
    EndIf
  EndIf
  If (numOutputs > 1)
    Line(64,-192,128,-192)
    FontSize(10)
    FText(112,-184,"y2")
    Line(-32,128,-96,128)
    FontSize(10)
    FText(-80,136,"L2")
    If (numOutputs> 2)
      Line(64,-160,128,-160)
      FontSize(10)
      FText(112,-152,"y3")
      Line(-32,160,-96,160)
      FontSize(10)
      FText(-80,168,"L3")
      If (numOutputs>3)
        Line(64,-128,128,-128)
        FontSize(10)
        FText(112,-120,"y4")
        Line(-32,192,-96,192)
        FontSize(10)
        FText(-80,200,"L4")
        If (numOutputs>4)
          Line(-32,224,-96,224)
          FontSize(10)
          FText(-80,232,"L5")
          Line(64,-96,128,-96)
          FontSize(10)
          FText(112,-88,"y5")
          If (numOutputs> 5)
            Line(64,-64,128,-64)
            FontSize(10)
            FText(112,-56,"y6")
            Line(-32,256,-96,256)
            FontSize(10)
            FText(-80,264,"L6")
            If (numOutputs>6)
              Line(-32,288,-96,288)
              FontSize(10)
              FText(-80,296,"L7")
              Line(64,-32,128,-32)
              FontSize(10)
              FText(120,-24,"y7")
              If (numOutputs>7)
                FontSize(10)
                FText(-80,328,"L8")
                Line(-32,320,-96,320)
                Line(64,0,128,0)
                FontSize(10)
                FText(120,8,"y8")
                If (numOutputs>8)
                  Line(-32,352,-96,352)
                  FontSize(10)
                  FText(-80,360,"L9")
                  Line(64,32,128,32)
                  FontSize(10)
                  FText(120,40,"y9")
                  If (numOutputs>9)
                    Line(-32,384,-96,384)
                    FontSize(10)
                    FText(-80,392,"L10")
                    Line(64,64,128,64)
                    FontSize(10)
                    FText(120,72,"y10")
                  EndIf
                EndIf
              EndIf
            EndIf
          EndIf
        EndIf
      EndIf
    EndIf
  EndIf

NODES:
  INITIALIZE_NODE Learning_rate = _dud_
  INITIALIZE_NODE input1        = _dud_
  INITIALIZE_NODE L1            = _dud_
  INITIALIZE_NODE foutput       = _dud_
  INITIALIZE_NODE lamm          = _dud_
  INITIALIZE_NODE input2        = _dud_
  INITIALIZE_NODE input3        = _dud_
  INITIALIZE_NODE input4        = _dud_
  INITIALIZE_NODE input5        = _dud_
  INITIALIZE_NODE input6        = _dud_
  INITIALIZE_NODE input7        = _dud_
  INITIALIZE_NODE input8        = _dud_
  INITIALIZE_NODE input9        = _dud_
  INITIALIZE_NODE input10       = _dud_
  INITIALIZE_NODE foutput2      = _dud_
  INITIALIZE_NODE L2            = _dud_
  INITIALIZE_NODE foutput3      = _dud_
  INITIALIZE_NODE L3            = _dud_
  INITIALIZE_NODE L4            = _dud_
  INITIALIZE_NODE foutput4      = _dud_
  INITIALIZE_NODE L5            = _dud_
  INITIALIZE_NODE foutput5      = _dud_
  INITIALIZE_NODE foutput6      = _dud_
  INITIALIZE_NODE L6            = _dud_
  INITIALIZE_NODE foutput7      = _dud_
  INITIALIZE_NODE L7            = _dud_
  INITIALIZE_NODE foutput8      = _dud_
  INITIALIZE_NODE L8            = _dud_
  INITIALIZE_NODE foutput9      = _dud_
  INITIALIZE_NODE L9            = _dud_
  INITIALIZE_NODE foutput10     = _dud_
  INITIALIZE_NODE L10           = _dud_

  Learning_rate -3 13 INPUT REAL
  input1 -3 -8 INPUT REAL
  L1 -3 3 INPUT REAL
  foutput 4 -7 OUTPUT REAL
  lamm -3 14 INPUT REAL
  #IF numInputs>1
    input2 -3 -7 INPUT REAL
    #IF numInputs>2
      input3 -3 -6 INPUT REAL
      #IF numInputs>3
        input4 -3 -5 INPUT REAL
        #IF numInputs>4
          input5 -3 -4 INPUT REAL
          #IF numInputs>5
            input6 -3 -3 INPUT REAL
            #IF numInputs>6
              input7 -3 -2 INPUT REAL
              #IF numInputs>7
                input8 -3 -1 INPUT REAL
                #IF numInputs>8
                  input9 -3 0 INPUT REAL
                  #IF numInputs>9
                    input10 -3 1 INPUT REAL
                  #END
                #END
              #END
            #END
          #END
        #END
      #END
    #END
  #END
  #IF numOutputs>1
    foutput2 4 -6 OUTPUT REAL
    L2 -3 4 INPUT REAL
    #IF numOutputs>2
      foutput3 4 -5 OUTPUT REAL
      L3 -3 5 INPUT REAL
      #IF numOutputs>3
        L4 -3 6 INPUT REAL
        foutput4 4 -4 OUTPUT REAL
        #IF numOutputs>4
          L5 -3 7 INPUT REAL
          foutput5 4 -3 OUTPUT REAL
          #IF numOutputs>5
            foutput6 4 -2 OUTPUT REAL
            L6 -3 8 INPUT REAL
            #IF numOutputs>6
              foutput7 4 -1 OUTPUT REAL
              L7 -3 9 INPUT REAL
              #IF numOutputs>7
                foutput8 4 0 OUTPUT REAL
                L8 -3 10 INPUT REAL
                #IF numOutputs>8
                  foutput9 4 1 OUTPUT REAL
                  L9 -3 11 INPUT REAL
                  #IF numOutputs>9
                    foutput10 4 2 OUTPUT REAL
                    L10 -3 12 INPUT REAL
                  #END
                #END
              #END
            #END
          #END
        #END
      #END
    #END
  #END

VALVE-GROUP:
  #IF  activation==LINEAR
    RTDS.CTL.RISC_CMODEL
        V4COMP: ModelName = InputLv2 SubSystem = $SS Processor = $ctrlGrp Priority = $Pri
        DATA_SIZES: INTDATA=4 FLOATDATA=0 CHARDATA_FRMT=0 INVARS=22 OUTVARS=10
        INTDATA: $numInputs numInputs $numOutputs numOutputs $activation activation $batch_size batch_size
        INVARS: $Learning_rate IEEE Learning_rate $input1 IEEE input1 $L1 IEEE L1 $input2 IEEE input2 $input3 IEEE input3
        INVARS: $input4 IEEE input4 $input5 IEEE input5 $input6 IEEE input6 $input7 IEEE input7 $input8 IEEE input8
        INVARS: $input9 IEEE input9 $input10 IEEE input10 $L2 IEEE L2 $L3 IEEE L3 $L4 IEEE L4
        INVARS: $L5 IEEE L5 $L6 IEEE L6 $L7 IEEE L7 $L8 IEEE L8 $L9 IEEE L9
        INVARS: $L10 IEEE L10 $lamm IEEE lamm
        OUTVARS: $foutput2 IEEE foutput2 $foutput3 IEEE foutput3 $foutput4 IEEE foutput4 $foutput5 IEEE foutput5 $foutput6 IEEE foutput6
        OUTVARS: $foutput7 IEEE foutput7 $foutput8 IEEE foutput8 $foutput9 IEEE foutput9 $foutput10 IEEE foutput10 $foutput IEEE foutput
    END

  #ELSEIF activation==TAH
    RTDS.CTL.RISC_CMODEL
        V4COMP: ModelName = InputLv2Tanh SubSystem = $SS Processor = $ctrlGrp Priority = $Pri
        DATA_SIZES: INTDATA=4 FLOATDATA=0 CHARDATA_FRMT=0 INVARS=22 OUTVARS=10
        INTDATA: $numInputs numInputs $numOutputs numOutputs $activation activation $batch_size batch_size
        INVARS: $Learning_rate IEEE Learning_rate $input1 IEEE input1 $L1 IEEE L1 $lamm IEEE lamm $input2 IEEE input2
        INVARS: $input3 IEEE input3 $input4 IEEE input4 $input5 IEEE input5 $input6 IEEE input6 $input7 IEEE input7
        INVARS: $input8 IEEE input8 $input9 IEEE input9 $input10 IEEE input10 $L2 IEEE L2 $L3 IEEE L3
        INVARS: $L4 IEEE L4 $L5 IEEE L5 $L6 IEEE L6 $L7 IEEE L7 $L8 IEEE L8
        INVARS: $L9 IEEE L9 $L10 IEEE L10
        OUTVARS: $foutput IEEE foutput $foutput2 IEEE foutput2 $foutput3 IEEE foutput3 $foutput4 IEEE foutput4 $foutput5 IEEE foutput5
        OUTVARS: $foutput6 IEEE foutput6 $foutput7 IEEE foutput7 $foutput8 IEEE foutput8 $foutput9 IEEE foutput9 $foutput10 IEEE foutput10
    END

  #ELSE 
  #END 
COMPONENT-DESCRIPTORS:
  CLASSIFICATION: GENERAL
  PHASES:         N/A
  MAINSTEP:       YES
  SUBSTEP:        NO
  SUPERSTEP:      NO
  SMALLDT:        NO
  GPES:           NO
  TWRT:           NO
  DIST:           NO
  DEPRECATED:     NO

