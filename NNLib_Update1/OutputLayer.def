Component Builder 2.0
COMPONENT-BUILDER-OPTIONS:
  COMPONENT-MODE:        CTL
  AUTO-UPDATE-FILE-DEFN: true
  SHOW-NODE-NAMES:       false
  DEFAULT-FONT-SIZE:     10

PARAMETERS:
  SECTION: "CONFIGURATION"
    ctrlGrp "Assigned Control Group" "" 0 INTEGER 1 1 54
    Pri     "Priority Level"         "" 0 INTEGER 1 1
  SECTION: "ParameterSelection"
    numInputs  "Description" ""                     0 INTEGER 1 1 6
    numOutputs "Description" ""                     0 INTEGER 1 1 2
    activation "Description" "LINEAR;TANH;RELU;SIG" 0 TOGGLE  1

GRAPHICS:
  Box(-32,-208,56,280)
  Line(56,-8,56,-8)
  Line(56,-8,56,-8)
  Line(56,-8,56,-8)
  Line(56,-8,56,-8)
  Line(-32,-32,-96,-32)
  Line(56,32,128,32)
  FontSize(10)
  FText(112,40,"Li1")
  Line(-32,224,-96,224)
  FText(112,-56,"y1")
  FontSize(10)
  FText(-72,240,"Learning Rate")
  FontSize(10)
  FText(-80,200,"Loss y1")
  Line(56,-64,128,-64)
  Line(-96,192,-32,192)
  FontSize(10)
  FText(-72,-24,"Input1")
  If (numInputs > 1)
    Line(-32,0,-96,0)
    FontSize(10)
    FText(-72,8,"Input 2")
    Line(56,64,128,64)
    FontSize(10)
    FText(112,72,"Li2")
    If (numInputs > 2)
      Line(-32,32,-96,32)
      FontSize(10)
      FText(-72,40,"Input3")
      Line(56,96,128,96)
      FontSize(10)
      FText(112,104,"Li3")
      If (numInputs>3)
        Line(-32,64,-96,64)
        FontSize(10)
        FText(-72,72,"input4")
        Line(56,128,128,128)
        FontSize(10)
        FText(112,136,"Li4")
        If (numInputs>4)
          Line(-32,96,-96,96)
          FontSize(10)
          FText(-72,104,"input5")
          Line(56,160,128,160)
          FontSize(10)
          FText(112,168,"Li5")
          If (numInputs>5)
            Line(-32,128,-96,128)
            FontSize(10)
            FText(-72,136,"input6")
            Line(56,192,128,192)
            FontSize(10)
            FText(112,200,"Li6")
          EndIf
        EndIf
      EndIf
    EndIf
  EndIf
  If (numOutputs > 1)
    Line(56,-96,128,-96)
    FontSize(10)
    FText(112,-88,"y2")
    FontSize(10)
    FText(-72,168,"Loss y2")
    Line(-96,160,-32,160)
    If (numOutputs> 2)
      Line(56,-128,128,-128)
      FontSize(10)
      FText(112,-120,"y3")
      If (numOutputs>3)
        Line(56,-160,128,-160)
        FontSize(10)
        FText(112,-152,"y4")
        If (numOutputs>4)
          Line(56,-192,128,-192)
          FontSize(10)
          FText(112,-184,"y5")
          If (numOutputs> 5)
            Line(56,-224,128,-224)
            FontSize(10)
            FText(112,-216,"y6")
          EndIf
        EndIf
      EndIf
    EndIf
  EndIf
  Line(-32,256,-96,256)
  FontSize(10)
  FText(-72,272,"lambda")

NODES:
  INITIALIZE_NODE label         = _dud_
  INITIALIZE_NODE Learning_rate = _dud_
  INITIALIZE_NODE foutput       = _dud_
  INITIALIZE_NODE Li1           = _dud_
  INITIALIZE_NODE input1        = _dud_
  INITIALIZE_NODE input2        = _dud_
  INITIALIZE_NODE Li2           = _dud_
  INITIALIZE_NODE input3        = _dud_
  INITIALIZE_NODE Li3           = _dud_
  INITIALIZE_NODE input4        = _dud_
  INITIALIZE_NODE Li4           = _dud_
  INITIALIZE_NODE input5        = _dud_
  INITIALIZE_NODE Li5           = _dud_
  INITIALIZE_NODE input6        = _dud_
  INITIALIZE_NODE Li6           = _dud_
  INITIALIZE_NODE foutput2      = _dud_
  INITIALIZE_NODE label2        = _dud_
  INITIALIZE_NODE foutput3      = _dud_
  INITIALIZE_NODE foutput4      = _dud_
  INITIALIZE_NODE foutput5      = _dud_
  INITIALIZE_NODE foutput6      = _dud_
  INITIALIZE_NODE lam           = _dud_

  label -3 6 INPUT REAL
  Learning_rate -3 7 INPUT REAL
  foutput 4 -2 OUTPUT REAL
  Li1 4 1 OUTPUT REAL
  input1 -3 -1 INPUT REAL
  #IF numInputs>1
    input2 -3 0 INPUT REAL
    Li2 4 2 OUTPUT REAL
    #IF numInputs>2
      input3 -3 1 INPUT REAL
      Li3 4 3 OUTPUT REAL
      #IF numInputs>3
        input4 -3 2 INPUT REAL
        Li4 4 4 OUTPUT REAL
        #IF numInputs>4
          input5 -3 3 INPUT REAL
          Li5 4 5 OUTPUT REAL
          #IF numInputs>5
            input6 -3 4 INPUT REAL
            Li6 4 6 OUTPUT REAL
          #END
        #END
      #END
    #END
  #END
  #IF numOutputs>1
    foutput2 4 -3 OUTPUT REAL
    label2 -3 5 INPUT REAL
    #IF numOutputs>2
      foutput3 4 -4 OUTPUT REAL
      #IF numOutputs>3
        foutput4 4 -5 OUTPUT REAL
        #IF numOutputs>4
          foutput5 4 -6 OUTPUT REAL
          #IF numOutputs>5
            foutput6 4 -7 OUTPUT REAL
          #END
        #END
      #END
    #END
  #END
  lam -3 8 INPUT REAL

VALVE-GROUP:
  #IF  activation==LINEAR
    RTDS.CTL.RISC_CMODEL
        V4COMP: ModelName = OutputLayerLinear SubSystem = $SS Processor = $ctrlGrp Priority = $Pri
        DATA_SIZES: INTDATA=3 FLOATDATA=0 CHARDATA_FRMT=0 INVARS=10 OUTVARS=12
        INTDATA: $numInputs numInputs $numOutputs numOutputs $activation activation
        INVARS: $label IEEE label $Learning_rate IEEE Learning_rate $input1 IEEE input1 $input2 IEEE input2 $input3 IEEE input3
        INVARS: $input4 IEEE input4 $input5 IEEE input5 $input6 IEEE input6 $label2 IEEE label2 $lam IEEE lam
        OUTVARS: $foutput IEEE foutput $Li1 IEEE Li1 $Li2 IEEE Li2 $Li3 IEEE Li3 $Li4 IEEE Li4
        OUTVARS: $Li5 IEEE Li5 $Li6 IEEE Li6 $foutput2 IEEE foutput2 $foutput3 IEEE foutput3 $foutput4 IEEE foutput4
        OUTVARS: $foutput5 IEEE foutput5 $foutput6 IEEE foutput6
    END

  #END 
COMPONENT-DESCRIPTORS:
  CLASSIFICATION: GENERAL
  PHASES:         N/A
  MAINSTEP:       YES
  SUBSTEP:        NO
  SUPERSTEP:      NO
  SMALLDT:        NO
  GPES:           NO
  TWRT:           NO
  DIST:           NO
  DEPRECATED:     NO

